{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "comma-dangle": ["error", "never"],
    "object-curly-spacing": ["error", "always"],
    "max-len": ["error", 120],
    "prefer-const": ["warn"],
    "eol-last": ["error", "always"]
  }
}